				"variable.other.enummember"
			],
			"settings": {
				"foreground": "#db4543"
			}
		},
		{
			"scope": [
				"constant.character.escape",
				"keyword.operator",
				"markup.underline.link",
				"string.regexp",
				"string.url"
			],
			"settings": {
				"foreground": "#4ac3d3"
